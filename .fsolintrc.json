{
  "rules": {
    "cases": {
      "severity": "error",
      "options": {
        "case": "kebab-case"
      }
    },
    "max-depth": {
      "severity": "error",
      "options": {
        "depth": 2
      }
    },
    "new-line-end-file": "warning",
    "no-dirs-extension": {
      "severity": "error",
      "options": {
        "invalidExtensions": [".js", ".jsx"]
      }
    },
    "no-duplicates": {
      "severity": "error"
    },
    "no-empty-dirs": "error",
    "no-empty-files": "warning",
    "project-structure": {
      "severity": "warning",
      "options": {
        "structure": {}
      }
    },
    "required-index": 1,
    "allowed-extensions": "warning"
  },
  "overrides": {
    "README.md|LICENSE": {
      "cases": 0
    }
  }
}
